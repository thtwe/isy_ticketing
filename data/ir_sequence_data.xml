<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!-- Sequences for sale.order -->
        <record id="seq_isy_ticketing_requests_maintenance" model="ir.sequence">
            <field name="name">ISY Ticketing Requests (Facility)</field>
            <field name="code">isy.ticketing.requests.maintenance</field>
            <field name="prefix">FYR%(year)s%(month)s%(day)s_</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">no_gap</field>
        </record>

